{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["./app.js", "./public", "./routes", "./test", "./views"],
    "includePattern": ".+\\.js$",
    "excludePattern": "(docs)"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "systemName": "SIGNU SERVER",
    "footer": "Created by Marcos Ruiz",
    "copyright": "2018",
    "navType": "vertical",
    "theme": "spacelab",
    "linenums": true,
    "collapseSymbols": true,
    "inverseNav": false
  },
  "opts": {
    "template": "./node_modules/docdash",
    "encoding": "utf8",
    "destination": "docs/js",
    "recurse": true,
    "private": true
  }
}